{
  dconf.settings = {
    "org/gnome/shell/extensions/paperwm" = {
      show-window-position-bar = false;
      gesture-enabled = false;
      default-focus-mode = 1;
      gesture-workspace-fingers = 0;
      selection-border-size = 0;
      selection-border-radius-top = 12;
      selection-border-radius-bottom = 12;
      window-gap = 99;
      vertical-margin = 40;
      vertical-margin-bottom = 40;
      disable-topbar-styling = true;
      show-focus-mode-icon = false;
      show-open-position-icon = false;
      maximize-within-tiling = true;
      maximize-width-percent = 0.7;
      minimap-scale = 0.0;
    };

    "org/gnome/shell/extensions/paperwm/keybindings" = {
      switch-left = [ "<super>h" ];
      switch-right = [ "<super>l" ];
      move-left = [ "<super><shift>h" ];
      move-right = [ "<super><shift>l" ];
      close-window = [ "<super>q" ];
      switch-first = [ "<super>1" ];
      switch-second = [ "<super>2" ];
      switch-third = [ "<super>3" ];
      switch-fourth = [ "<super>4" ];
      switch-fifth = [ "<super>5" ];
      switch-sixth = [ "<super>6" ];
      switch-seventh = [ "<super>7" ];
      switch-eighth = [ "<super>8" ];
      switch-ninth = [ "<super>9" ];
      switch-tenth = [ "<super>0" ];
      take-window = [ "" ];
    };
  };
}
